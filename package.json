{
  "name": "test-runner",
  "author": "Lloyd Brookes <75pound@gmail.com>",
  "version": "0.1.5",
  "description": "Minimal test runner",
  "repository": "https://github.com/75lb/test-runner.git",
  "license": "MIT",
  "main": "src/main.js",
  "bin": "src/bin.js",
  "keywords": [],
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "tape test/*.js",
    "docs": "jsdoc2md -t jsdoc2md/README.md lib/*.js > README.md; echo",
