  },
  "dependencies": {
    "ansi-escape-sequences": "^2.2.2",
    "command-line-tool": "~0.6.0",
    "feature-detect-es6": "^1.3.1",
    "file-set": "^1.0.1",
    "typical": "^2.5.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0"
  }
}
